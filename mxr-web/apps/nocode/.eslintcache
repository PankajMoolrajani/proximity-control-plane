}></Frame>\n        </Viewport>\n      </Editor>\n    </div>\n  )\n}\n\nexport default PageBuilder\n","/mxr-web/apps/nocode/src/layouts/components/AppBar.react.js",[],"/mxr-web/apps/nocode/src/layouts/components/Sidebar.react.js",[],"/mxr-web/apps/nocode/src/layouts/page.react.js",[],"/mxr-web/apps/nocode/src/pages/data-sources/components/list.react.js",[],"/mxr-web/apps/nocode/src/pages/data-sources/components/create.react.js",[],"/mxr-web/apps/nocode/src/pages/page-builder/components/selectors/index.js",[],"/mxr-web/apps/nocode/src/pages/page-builder/components/selectors/Custom1/index.js",[],"/mxr-web/apps/nocode/src/pages/page-builder/components/selectors/Custom3/index.js",[],"/mxr-web/apps/nocode/src/pages/page-builder/components/selectors/Custom2/index.js",["182","183"],"import React from 'react'\nimport { Container } from '../Container'\nimport { Element, useNode } from '@craftjs/core'\nimport { Video } from '../Video'\n\nexport const Custom2VideoDrop = ({ children }) => {\n  const {\n    connectors: { connect }\n  } = useNode()\n  return (\n    <div ref={connect} className=\"flex-1 ml-5 h-full\">\n      {children}\n    </div>\n  )\n}\nCustom2VideoDrop.craft = {\n  rules: {\n    canMoveIn: (incoming, self, helper) => {\n      return (\n        incoming.data.type == Video && helper(self.id).decendants().length == 0\n      )\n    }\n  }\n}\n\nexport const Custom2 = (props) => {\n  return (\n    <Container {...props} className=\"overflow-hidden\">\n      <div className=\"w-24\">\n        <h2 className=\"text-xs text-white\">\n          You can only drop\n          <br />\n          one video here.\n        </h2>\n      </div>\n      <Element canvas is={Custom2VideoDrop} id=\"wow\">\n        <Video />\n      </Element>\n    </Container>\n  )\n}\n\nCustom2.craft = {\n  ...Container.craft,\n  displayName: 'Custom 2'\n}\n","/mxr-web/apps/nocode/src/pages/page-builder/components/selectors/Button/index.js",["184","185","186","187"],"import React from 'react'\nimport { UserComponent, useNode } from '@craftjs/core'\nimport { Text } from '../Text'\nimport { ButtonSettings } from './ButtonSettings'\nimport styled from 'styled-components'\nimport cx from 'classnames'\n\nconst StyledButton = styled.button`\n  background: ${(props) =>\n    props.buttonStyle == 'full'\n      ? `rgba(${Object.values(props.background)})`\n      : 'transparent'};\n  border: 2px solid transparent;\n  border-color: ${(props) =>\n    props.buttonStyle == 'outline'\n      ? `rgba(${Object.values(props.background)})`\n      : 'transparent'};\n  margin: ${({ margin }) =>\n    `${margin[0]}px ${margin[1]}px ${margin[2]}px ${margin[3]}px`};\n`\n\nexport const Button = (props) => {\n  const {\n    connectors: { connect }\n  } = useNode((node) => ({\n    selected: node.events.selected\n  }))\n  const { text, textComponent, color, ...otherProps } = props\n  return (\n    <StyledButton\n      ref={connect}\n      className={cx([\n        'rounded w-full px-4 py-2',\n        {\n          'shadow-lg': props.buttonStyle == 'full'\n        }\n      ])}\n      {...otherProps}\n    >\n      <Text {...textComponent} text={text} color={props.color} />\n    </StyledButton>\n  )\n}\nButton.craft = {\n  displayName: 'Button',\n  props: {\n    background: { r: 255, g: 255, b: 255, a: 0.5 },\n    color: { r: 92, g: 90, b: 90, a: 1 },\n    buttonStyle: 'full',\n    text: 'Button',\n    margin: ['5', '0', '5', '0'],\n    textComponent: Object.assign(Object.assign({}, Text.craft.props), {\n      textAlign: 'center'\n    })\n  },\n  related: {\n    toolbar: ButtonSettings\n  }\n}\n","/mxr-web/apps/nocode/src/pages/page-builder/components/selectors/Video/index.js",[],"/mxr-web/apps/nocode/src/pages/page-builder/components/editor/index.js",[],"/mxr-web/apps/nocode/src/pages/page-builder/components/editor/RenderNode.js",["188","189"],"import React, { useEffect, useRef, useCallback } from 'react'\nimport { useNode, useEditor } from '@craftjs/core'\nimport styled from 'styled-components'\nimport ArrowUp from '@material-ui/icons/ArrowUpward'\nimport Move from '@material-ui/icons/OpenWith'\nimport Delete from '@material-ui/icons/Delete'\nimport ReactDOM from 'react-dom'\nimport { ROOT_NODE } from '@craftjs/utils'\n\nconst IndicatorDiv = styled.div`\n  height: 30px;\n  margin-top: -29px;\n  font-size: 12px;\n  line-height: 12px;\n\n  svg {\n    fill: #fff;\n    width: 15px;\n    height: 15px;\n  }\n`\nconst Btn = styled.a`\n  padding: 0 0px;\n  opacity: 0.9;\n  display: flex;\n  align-items: center;\n  > div {\n    position: relative;\n    top: -50%;\n    left: -50%;\n  }\n`\nexport const RenderNode = ({ render }) => {\n  const { actions, query, connectors } = useEditor()\n  const {\n    id,\n    isActive,\n    isHover,\n    dom,\n    name,\n    moveable,\n    deletable,\n    connectors: { drag },\n    parent\n  } = useNode((node) => ({\n    isActive: node.events.selected,\n    isHover: node.events.hovered,\n    dom: node.dom,\n    name: node.data.custom.displayName || node.data.displayName,\n    moveable: query.node(node.id).isDraggable(),\n    deletable: query.node(node.id).isDeletable(),\n    parent: node.data.parent,\n    props: node.data.props\n  }))\n  const currentRef = useRef()\n  useEffect(() => {\n    if (dom) {\n      if (isActive || isHover) dom.classList.add('component-selected')\n      else dom.classList.remove('component-selected')\n    }\n  }, [dom, isActive, isHover])\n  const getPos = useCallback((dom) => {\n    const { top, left, bottom } = dom\n      ? dom.getBoundingClientRect()\n      : { top: 0, left: 0, bottom: 0 }\n    return {\n      top: `${top > 0 ? top : bottom}px`,\n      left: `${left}px`\n    }\n  }, [])\n  const scroll = useCallback(() => {\n    const { current: currentDOM } = currentRef\n    if (!currentDOM) return\n    const { top, left } = getPos(dom)\n    currentDOM.style.top = top\n    currentDOM.style.left = left\n  }, [dom])\n  useEffect(() => {\n    document\n      .querySelector('.craftjs-renderer')\n      .addEventListener('scroll', scroll)\n    return () => {\n      if (document.querySelector('.craftjs-renderer')) {\n        document\n          .querySelector('.craftjs-renderer')\n          .removeEventListener('scroll', scroll)\n      }\n    }\n  }, [scroll])\n  return (\n    <>\n      {isHover || isActive\n        ? ReactDOM.createPortal(\n            <IndicatorDiv\n              ref={currentRef}\n              className=\"px-2 py-2 text-white bg-primary fixed flex items-center\"\n              style={{\n                left: getPos(dom).left,\n                top: getPos(dom).top,\n                zIndex: 9999\n              }}\n            >\n              <h2 className=\"flex-1 mr-4\">{name}</h2>\n              {moveable ? (\n                <Btn className=\"mr-2 cursor-move\" ref={drag}>\n                  <Move />\n                </Btn>\n              ) : null}\n              {id !== ROOT_NODE && (\n                <Btn\n                  className=\"mr-2 cursor-pointer\"\n                  onClick={() => {\n                    actions.selectNode(parent)\n                  }}\n                >\n                  <ArrowUp />\n                </Btn>\n              )}\n              {deletable ? (\n                <Btn\n                  className=\"cursor-pointer\"\n                  onMouseDown={(e) => {\n                    e.stopPropagation()\n                    actions.delete(id)\n                  }}\n                >\n                  <Delete />\n                </Btn>\n              ) : null}\n            </IndicatorDiv>,\n            document.body\n          )\n        : null}\n      {render}\n    </>\n  )\n}\n","/mxr-web/apps/nocode/src/pages/page-builder/components/selectors/Video/VideoSettings.js",[],"/mxr-web/apps/nocode/src/pages/page-builder/components/selectors/Container/index.js",["190","191"],"import React from 'react'\nimport { Resizer } from '../Resizer'\nimport { ContainerSettings } from './ContainerSettings'\n\nconst defaultProps = {\n  flexDirection: 'column',\n  alignItems: 'flex-start',\n  justifyContent: 'flex-start',\n  fillSpace: 'no',\n  padding: ['0', '0', '0', '0'],\n  margin: ['0', '0', '0', '0'],\n  background: { r: 255, g: 255, b: 255, a: 1 },\n  color: { r: 0, g: 0, b: 0, a: 1 },\n  shadow: 0,\n  radius: 0,\n  width: '100%',\n  height: 'auto'\n}\nexport const Container = (props) => {\n  props = {\n    ...defaultProps,\n    ...props\n  }\n  const {\n    flexDirection,\n    alignItems,\n    justifyContent,\n    fillSpace,\n    background,\n    color,\n    padding,\n    margin,\n    shadow,\n    radius,\n    children\n  } = props\n  return (\n    <Resizer\n      propKey={{ width: 'width', height: 'height' }}\n      style={{\n        justifyContent,\n        flexDirection,\n        alignItems,\n        background: `rgba(${Object.values(background)})`,\n        color: `rgba(${Object.values(color)})`,\n        padding: `${padding[0]}px ${padding[1]}px ${padding[2]}px ${padding[3]}px`,\n        margin: `${margin[0]}px ${margin[1]}px ${margin[2]}px ${margin[3]}px`,\n        boxShadow:\n          shadow == 0\n            ? 'none'\n            : `0px 3px 100px ${shadow}px rgba(0, 0, 0, 0.13)`,\n        borderRadius: `${radius}px`,\n        flex: fillSpace == 'yes' ? 1 : 'unset'\n      }}\n    >\n      {children}\n    </Resizer>\n  )\n}\nContainer.craft = {\n  displayName: 'Container',\n  props: defaultProps,\n  rules: {\n    canDrag: () => true\n  },\n  related: {\n    toolbar: ContainerSettings\n  }\n}\n","/mxr-web/apps/nocode/src/pages/page-builder/components/selectors/Text/index.js",[],"/mxr-web/apps/nocode/src/pages/page-builder/components/editor/Viewport/index.js",["192"],"import React, { useState, useEffect } from 'react'\nimport cx from 'classnames'\nimport { useEditor } from '@craftjs/core'\nimport { Toolbox } from './Toolbox'\nimport { Sidebar } from './Sidebar'\nimport { Header } from './Header'\nimport {\n  Button as MaterialButton,\n  Dialog,\n  DialogTitle,\n  DialogContent,\n  DialogAc